in: t
out: d

me = mean_anomaly(Craft.Ap + Parent.R, Craft.Pe + Parent.R, Craft.TrueAnomaly * (pi/180), t)
target = true_anomaly(Target.Ap + Parent.R, Target.Pe + Parent.R, Target.TrueAnomaly * (pi/180), t)

me = (Craft.ω * (pi/180) + Craft.Ω * (pi/180) + me)
target =  (Target.ω * (pi/180) + Target.Ω * (pi/180) + target)

d = wrap(target - me) * (180/pi)